<%# Photo gallery component for the show page %>
<div class="photo-gallery h-100" data-controller="lightbox">
  <% photos.each do |photo| %>
    <%= link_to url_for(photo) do %>
      <%= cl_image_tag photo.key, width: 400, crop: :fill, class: 'photo-gallery-item' %>
    <% end %>
  <% end %>
</div>
